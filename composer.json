{
    "name": "campelo/laravel-make-full",
    "description": "Generate complete CRUD structure: Model, Controller, Service, Repository, Resource, Requests, Policy, Migration, Factory, Seeder - all from a single command",
    "keywords": ["laravel", "generator", "crud", "scaffolding", "make", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Campelo",
            "email": "jose.campelo@soffia.co"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Campelo\\MakeFull\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Campelo\\MakeFull\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Campelo\\MakeFull\\MakeFullServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
